{
  "name": "toggle-eslint",
  "displayName": "toggle-eslint",
  "description": "Toggle ESLint",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Linters"
  ],
  "activationEvents": [
    "onCommand:toggle-eslint.toggle"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "toggle-eslint.toggle",
        "title": "Toggle ESLint"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.63.0",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4",
    "@vscode/test-electron": "^1.6.2"
  },
  "publisher": "zentus"
}
